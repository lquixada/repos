{
  "name": "repos",
  "version": "3.1.17",
  "private": true,
  "description": "An explorer app for projects in Github.",
  "author": "Leonardo Quixada <lquixada@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prelambda:deploy": "npm run -s build",
    "preprod": "npm run -s build",
    "preversion": "npm test",
    "build": "npm run -s build:babel && npm run -s build:webpack",
    "build:analyze": "./tasks/analyze",
    "build:babel": "babel --extensions '.ts,.tsx' -D src -d dist",
    "build:webpack": "webpack-cli -p --env.prod",
    "deploy:major": "npm version major && git push --follow-tags",
    "deploy:minor": "npm version minor && git push --follow-tags",
    "deploy:patch": "npm version patch && git push --follow-tags",
    "dev": "pm2 start --no-daemon",
    "specs": "jest --coverage",
    "lambda:deploy": "serverless deploy -v",
    "lint": "tslint './src/**/*.{ts,tsx}'",
    "prod": "NODE_ENV=production serverless offline --noEnvironment --skipCacheInvalidation --dontPrintOutput",
    "ssl:build": "./tasks/ssl",
    "test": "npm run -s specs && npm run -s lint"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix './src/**/*.{ts,tsx}'",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@octokit/rest": "15.10.0",
    "apollo-boost": "0.1.15",
    "body-parser": "1.18.3",
    "compression": "1.7.4",
    "cross-fetch": "2.2.2",
    "dotenv": "6.0.0",
    "express": "4.16.4",
    "graphql-tag": "2.10.1",
    "graphql-yoga": "1.17.4",
    "helmet": "3.16.0",
    "history": "4.9.0",
    "morgan": "1.9.1",
    "nprogress": "0.2.0",
    "pm2": "3.4.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-github-fork-ribbon": "0.6.0",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.8.1",
    "react-octicons": "0.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "serverless-http": "1.9.1",
    "styled-components": "3.4.5",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/enzyme": "3.9.1",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-helmet": "5.0.8",
    "@types/react-router-config": "1.1.2",
    "@types/react-router-dom": "4.3.1",
    "@types/webpack-env": "1.13.9",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-schema": "1.2.2",
    "assets-webpack-plugin": "3.9.10",
    "babel-core": "^7.0.0-0",
    "babel-jest": "24.5.0",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "0.1.19",
    "codecov": "3.2.0",
    "copy-webpack-plugin": "4.5.2",
    "deepmerge": "2.1.1",
    "dotenv-webpack": "1.7.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "enzyme-to-json": "3.3.5",
    "flux-standard-action": "2.0.4",
    "graphql-tools": "3.1.1",
    "husky": "1.3.1",
    "jest": "24.5.0",
    "lint-staged": "7.2.2",
    "lodash.get": "4.4.2",
    "lodash.merge": "4.6.1",
    "react-test-renderer": "16.8.6",
    "regenerator-runtime": "0.12.1",
    "serverless": "1.40.0",
    "serverless-offline": "3.25.9",
    "serverless-s3-deploy": "0.7.0",
    "supertest": "3.1.0",
    "tslint": "5.14.0",
    "typescript": "3.4.1",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.6.1",
    "webpack-hot-middleware": "2.24.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/repos/"
  }
}
